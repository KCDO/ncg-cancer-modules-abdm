        "medicationCodeableConcept": {
          "coding": [
            {
              "code": "18629005",
              "system": "http://snomed.info/sct",
              "display": "Administration of drug or medicament (procedure)"
            }
          ]
        },
        "dosageInstruction": [
          {
            "text": "Pain Management: Tylenol (Acetaminophen) 500 mg, every 6 hours as needed for pain. Percocet (Oxycodone/Acetaminophen) 5/325 mg, every 6 hours as needed for severe pain.\n    Antibiotics: Keflex (Cephalexin) 500 mg, every 6 hours for 7 days to prevent infection.\n    Other Medications: Colace (Docusate Sodium) 100 mg, twice daily to prevent constipation."
          }
        ]
      }
